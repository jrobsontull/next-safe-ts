{
  "name": "next-safe-ts",
  "version": "0.1.0",
  "description": "Typesafe library for adding security header defaults to Next.js projects",
  "keywords": [
    "next.js",
    "security",
    "headers",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jrobsontull/next-safe-ts"
  },
  "bugs": {
    "url": "https://github.com/jrobsontull/next-safe-ts/issues"
  },
  "homepage": "https://github.com/jrobsontull/next-safe-ts#readme",
  "author": "jrobsontull",
  "license": "BSD-3-Clause",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && rollup -c rollup.config.mjs",
    "lint": "eslint \"src/*.ts\"",
    "format": "prettier --config .prettierrc --write \"src/*.ts\"",
    "test": "jest"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^9.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {},
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/test/.*.(test|spec)).(ts|tsx|js)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
